<div id="pins" class="transitions-enable clearfix masonry">
	<% @pins.each do |pin| %>
		<div class="pin panel panel-default">
			<div class="panel panel-body">
				<img src="<%= pin.image_url %>" alt="<%= pin.title %>">
				<h2><%= pin.title %></h2>
				<p class="user">Publicado por <%= pin.user.email %></p>
			</div>
		</div>
	<% end %>	
</div>